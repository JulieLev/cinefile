<%= link_to "Add Cinema", new_cinema_path %> <br>

<% if @cinemas.any? %>
  <% @cinemas.each do |cinema| %><br>
    <%= cinema.name %><br>
    <%= cinema.address%>, <%= cinema.postcode.capitalize %><br>
